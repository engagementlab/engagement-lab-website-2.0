    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "prerender:express": "node content/bin/www.js",
    "prerender:compile": "ng run elab-web:prerender --configuration=production",
    "serve:content": "cd content; nodemon;",
    "serve:content:prod": "cd content; nodemon prod;",
    "serve:ssr": "npm-run-all -p serve:content:prod scully",
    "scully": "scully",
    "scully:serve": "scully serve"
