{
  "name": "engagement-lab-website-2.0",
  "version": "2.4.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --vendorChunk=true",
    "build": "ng build --configuration=production --progress",
    "build-qa": "ng build --configuration=qa --progress",
    "build-static": "npm-run-all -p serve:api:prod scully:build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "supportedBrowsers": "echo \"module.exports = $(browserslist-useragent-regexp --allowHigherVersions);\" > supported-browsers.js",
    "kill-node": "killall node",
    "dev": "npm-run-all -p serve:api start",
    "serve:http": "http-server dist/browser",
    "serve:api": "cd ../api; nodemon;",
    "serve:api:prod": "cd ../api; nodemon prod;",
    "serve:static": "npm-run-all -p serve:api:prod scully:build",
    "scully": "scully",
    "scully:build": "scully --sr --RSD --sge --nw --killServer",
    "scully:serve": "scully serve"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~9.0.4",
    "@angular/common": "~9.0.4",
